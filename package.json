{
  "version": "0.0.0",
  "name": "nuxt-boilerplate",
  "description": "Nuxt 3 Boilerplate",
  "homepage": "https://nuxt.defdev.xyz",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "eslint --fix --ext .ts,.js,.vue .",
    "test": "vitest --config vitest.config.ts",
    "test-coverage": "vitest --config vitest.config.ts --coverage",
    "cm": "git add . && cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.8.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "^3.1.0",
    "@stylistic/eslint-plugin-ts": "^1.4.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-istanbul": "^1.0.1",
    "@vitest/coverage-v8": "^1.0.1",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "nuxt": "^3.8.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "unplugin-vue-components": "^0.26.0",
    "vitest": "^1.0.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
